#!/bin/bash
 
cd /srv/scratch/IsoQuant/
 
python isoquant.py --data_type nanopore --threads 8 --bam_list ../isoquant_files/isoquant_bam_file_list.txt --reference ../gencode_v45/GRCh38.primary_assembly.genome.fa --genedb ../gencode_v45/gencode.v45.annotation.gtf --output ../isoquant_output
 
